import requests

# URL de la p치gina que queremos obtener
url = "https://www.example.com"

# Realizar la solicitud HTTP
respuesta = requests.get(url)

# Mostrar el c칩digo HTML obtenido
if respuesta.status_code == 200:
    print("HTML de la p치gina:")
    print(respuesta.text[:500])  # Mostramos solo los primeros 500 caracteres
else:
    print(f"Error al obtener la p치gina: {respuesta.status_code}")
